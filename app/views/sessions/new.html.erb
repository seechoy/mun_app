<h1>SIGN IN</h1>

<%= form_for(:session, :url => sessions_path) do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>
  <div class="actions">
    <%= f.submit "SIGN IN" %>
  </div>
<% end %>

<p>NEW USER?! <%= link_to "SIGN UP NOW!!!!", signup_path %></p>
